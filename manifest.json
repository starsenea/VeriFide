{
  "manifest_version": 3,
  "name": "VeriFide",
  "version": "1.0",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "ai"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  }
}